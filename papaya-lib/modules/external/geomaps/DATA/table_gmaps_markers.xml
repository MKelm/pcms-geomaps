<?xml version="1.0" encoding="ISO-8859-1" ?>
<table name="gmaps_markers" prefix="yes">
<fields>
<field name="marker_id" type="integer" size="4" null="no" autoinc="yes"/>
<field name="marker_folder" type="integer" size="4" null="no"/>
<field name="marker_title" type="string" size="200" null="no"/>
<field name="marker_desc" type="string" size="65535" null="no"/>
<field name="marker_address" type="string" size="255" null="no"/>
<field name="marker_lat" type="float" size="9,6" null="no"/>
<field name="marker_lng" type="float" size="9,6" null="no"/>
<field name="marker_sort" type="integer" size="2" null="no"/>
</fields>
<keys>
<primary-key><field>marker_id</field></primary-key>
<key name="marker_folder"><field>marker_folder</field></key>
</keys>
</table>
